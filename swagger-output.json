              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "anuncio": {
                      "$ref": "#/components/schemas/Anuncio"
                    }
                  }
                }
              }
            }
          },
          "201": {
            "description": "Created"
          },
          "404": {
            "description": "Anúncio não encontrado."
          },
          "500": {
            "description": "Erro interno do servidor."
          }
        }
      },
      "delete": {
        "tags": [
          "Anúncios"
        ],
        "summary": "Excluir um anúncio",
        "description": "Remove um anúncio do sistema pelo seu ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID do Anúncio"
          },
          {
            "name": "authorization",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Anúncio excluído com sucesso.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Anúncio excluído com sucesso."
                    },
                    "anuncio": {
                      "$ref": "#/components/schemas/Anuncio"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Anúncio não encontrado."
          },
          "500": {
            "description": "Erro interno do servidor."
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
