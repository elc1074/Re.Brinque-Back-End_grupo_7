{
  "openapi": "3.0.3",
  "info": {
    "title": "API Re-Brinque",
    "description": "Documentação da API do projeto Re-Brinque, para gerenciamento de usuários, anúncios de brinquedos e transações.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Servidor de Desenvolvimento"
    }
  ],
  "tags": [
    {
      "name": "Autenticação",
      "description": "Endpoints para registro, login e gerenciamento de sessão."
    },
    {
      "name": "Anúncios",
      "description": "Endpoints para gerenciar os anúncios de brinquedos."
    },
    {
      "name": "Configuração",
      "description": "Endpoints de configurações de serviços externos."
    }
  ],
  "paths": {
    "/api/perfil": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        }
      }
    },
    "/api/auth/register": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Registrar um novo usuário",
        "description": "Cria uma nova conta de usuário com nome, e-mail e senha.",
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "409": {
            "description": "Conflict"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "requestBody": {}
      }
    },
    "/api/auth/login": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Login de usuário",
        "description": "Autentica um usuário com e-mail e senha e retorna um token JWT.",
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "requestBody": {}
      }
    },
    "/api/auth/sync": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Sincronizar conta de usuário com o Google via authorization_code",
        "description": "Recebe o código de autorização do Google, troca por token, busca dados do usuário e registra ou autentica.",
        "responses": {
          "200": {
            "description": "Login Google realizado com sucesso"
          },
          "400": {
            "description": "Erro na autenticação Google"
          },
          "500": {
            "description": "Erro interno no servidor"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "string"
                  },
                  "redirect_uri": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/google-callback": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Callback de Autenticação Google",
        "description": "Processa o token de credencial do Google para login ou registro.",
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "requestBody": {}
      }
    },
    "/api/anuncios/": {
      "post": {
        "tags": [
          "Anúncios"
        ],
        "summary": "Criar um novo anúncio",
        "description": "Cria um novo anúncio associado a um usuário, incluindo imagens.",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Anúncio criado com sucesso.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "anuncio": {
                      "$ref": "#/components/schemas/Anuncio"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Campos obrigatórios faltando."
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Erro interno do servidor."
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CorpoCriarAnuncio"
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Anúncios"
        ],
        "summary": "Listar e filtrar anúncios",
        "description": "Retorna uma lista de todos os anúncios, com a possibilidade de aplicar filtros.",
        "parameters": [
          {
            "name": "titulo",
            "in": "query",
            "description": "Filtrar por título (busca parcial)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "categoria_id",
            "in": "query",
            "description": "Filtrar por ID da categoria",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "tipo",
            "in": "query",
            "description": "Filtrar por tipo",
            "enum": [
              "TROCA",
              "DOACAO"
            ],
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "condicao",
            "in": "query",
            "description": "Filtrar por condição",
            "enum": [
              "NOVO",
              "SEMINOVO",
              "USADO"
            ],
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "marca",
            "in": "query",
            "description": "Filtrar por marca (busca parcial)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "description",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "content",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Lista de anúncios retornada com sucesso.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "anuncios": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Anuncio"
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Erro interno do servidor."
          }
        }
      }
    },
    "/api/anuncios/{id}": {
      "put": {
        "tags": [
          "Anúncios"
