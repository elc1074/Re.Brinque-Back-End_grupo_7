        "summary": "Obter anúncio por ID",
        "description": "Retorna os detalhes de um anúncio específico pelo seu ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID do Anúncio"
          }
        ],
        "responses": {
          "200": {
            "description": "Anúncio encontrado com sucesso.",
