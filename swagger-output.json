            "schema": {
              "type": "string"
            },
            "description": "ID do Anúncio"
          },
          {
            "name": "authorization",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Anúncio atualizado com sucesso.",
